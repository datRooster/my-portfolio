{
  "name": "my-portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "clean": "find . -name '.DS_Store' -delete && find . -name 'test-*.mjs' -delete",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "build:static": "echo 'Static export disabled - use hosting with Node.js support'",
    "deploy:static": "./scripts/deploy.sh static",
    "deploy:server": "./scripts/deploy.sh server",
    "deploy:prod": "NODE_ENV=production npm run build && npm run db:migrate:deploy",
    "deploy:vercel": "vercel --prod",
    "db:migrate:deploy": "prisma migrate deploy",
    "vercel:env": "vercel env add",
    "pm2:start": "pm2 start scripts/ecosystem.config.js",
    "pm2:stop": "pm2 stop portfolio-therooster",
    "pm2:restart": "pm2 restart portfolio-therooster",
    "pm2:logs": "pm2 logs portfolio-therooster",
    "backup:db": "pg_dump $DATABASE_URL > backup_$(date +%Y%m%d_%H%M%S).sql"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/qrcode": "^1.5.5",
    "@types/speakeasy": "^2.0.10",
    "bcryptjs": "^3.0.2",
    "crypto-js": "^4.2.0",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.545.0",
    "next": "15.5.4",
    "node-fetch": "^3.3.2",
    "otplib": "^12.0.1",
    "prisma": "^6.17.1",
    "qrcode": "^1.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "speakeasy": "^2.0.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}
